    "",
    "def mode1(n: int):",
    "  for i in range(n*n):",
    "    num = n*(n-i//n-1) + i % n",
    "    print(\"%3d\"%(num), end=\"\" if i%n != n-1 else None)",
    "",
    "def mode2(n: int):",
    "  for i in range(n*n):",
    "    num = n*(n-1-i%n) + i//n",
