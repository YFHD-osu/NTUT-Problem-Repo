    "def execM(m: int, n: int) -> None:",
    "  if m == 0:",
    "    return mode0(n)",
    "  ",
    "  if m == 1:",
    "    return mode1(n)",
    "  ",
    "  if m == 2:",
    "    return mode2(n)",
    "  ",
    "  return print(\"ERROR\")",
