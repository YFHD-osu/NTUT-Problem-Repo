    "def triangle(n: int) -> None:",
    "  for i in range(n):",
    "    print(\"#\" * (n-i-1) + \"*\" * (2*i+1))",
    "",
    "  return ",
    "",
    "def square(n: int) -> None:",
    "  print(\"*\" * n)",
    "  for i in range(n-2):",
    "    print(\"*\" + \"#\" * (n-2) + \"*\")",
    "  print(\"*\" * n)",
    "  return ",
    "",
    "def main() -> None:",
    "  n = int(input())",
    "",
    "  if n % 2:",
    "    return triangle(n)",
    "",
    "  return square(n)",
