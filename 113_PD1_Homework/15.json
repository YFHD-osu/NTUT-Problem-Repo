    "def main() -> None:",
    "  N = input()",
    "",
    "  valid = [i for i in N if i.isdigit()]",
    "",
    "  if not len(valid):",
    "    print([])",
    "    return",
    "",
    "  N = ''.join(valid)",
    "  valid = [int(i) for i in valid]",
    "  ",
    "  if N == N[::-1]:",
