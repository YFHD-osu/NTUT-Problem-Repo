    "  valid.sort(reverse=True)",
    "  print(fetchDup(valid))",
    "",
    "def removeDup(a: list) -> list:",
    "  seen = []",
    "  for i in a:",
    "    if i in seen: continue",
    "    seen.append(i)",
    "",
    "  return seen",
    "",
    "def fetchDup(a: list) -> list:",
    "  result = []",
    "  for i in a:",
    "    if i in result: continue",
    "    if a.count(i) < 2: continue",
    "    result.append(i)   ",
    "",
    "  return result",
    "",
    "if __name__ == \"__main__\":",
    "  main()"
