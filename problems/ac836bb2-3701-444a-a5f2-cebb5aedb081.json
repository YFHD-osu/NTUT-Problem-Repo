  "sampleCode": [
    "def main():",
    "  n = int(input())",
    "  ",
    "  if n<=0:",
    "    print(\"Error\")",
    "    return",
    "",
    "  forward(n-1)",
    "  backward(n-1)",
    "  return",
    "",
    "def forward(n: int):",
    "  for i in range(n):",
    "    star(n-i)",
    "    posCount(i)",
    "    revCount(i)",
    "    print()",
    "",
    "def backward(n: int):",
    "  for i in range(n+1):",
    "    star(i)",
    "    posCount(n-i)",
    "    revCount(n-i)",
    "    print()",
    "",
    "def posCount(n: int):",
    "  for i in range(1, 2*n+2, 2):",
    "    print(i, end=\"\")",
    "",
    "def revCount(n: int):",
    "  for i in range(2*n-1, 0, -2):",
    "    print(i, end=\"\")",
    "",
    "def star(n: int):",
    "  print(\"*\"*n, end=\"\")",
    "",
    "if __name__ == \"__main__\":",
    "  main()"
  ]
}